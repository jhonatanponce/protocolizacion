{% extends '::base.html.twig' %}

{% block body -%}
    {{ parent() }}
<div class="panel panel-default">
    
    <div class="panel-heading">
        <span class="glyphicon glyphicon-record"></span>
        <h3 class="panel-title" style="display: inline;">Detalle Desarrollo Habitacional</h3>
    </div>
    
    <div id="yw0" class="panel-body table-responsive">
        
        <table class="records_list table table-bordered table-striped table-hover" border="1">
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ entity.id }}</td>
                </tr>
                <tr>
                    <th>Av. / Calle / Esq. / Carr.</th>
                    <td>{{ entity.avCallEsqCarr }}</td>
                </tr>
                <tr>
                    <th>Coordenadas</th>
                    <td>{{ entity.coordenadas }}</td>
                </tr>
                <tr>
                    <th>Descripcion</th>
                    <td>{{ entity.descripcion }}</td>
                </tr>
                <tr>
                    <th>Fecha actualizacion</th>
                    <td>{{ entity.fechaActualizacion|date('Y-m-d H:i:s') }}</td>
                </tr>
                <tr>
                    <th>Fecha creaci√≥n</th>
                    <td>{{ entity.fechaCreacion|date('Y-m-d H:i:s') }}</td>
                </tr>
                <tr>
                    <th>Fecha transferencia</th>
                    <td>{{ entity.fechaTransferencia|date('Y-m-d H:i:s') }}</td>
                </tr>
                <tr>
                    <th>Lindero este</th>
                    <td>{{ entity.linderoEste }}</td>
                </tr>
                <tr>
                    <th>Lindero norte</th>
                    <td>{{ entity.linderoNorte }}</td>
                </tr>
                <tr>
                    <th>Lindero oeste</th>
                    <td>{{ entity.linderoOeste }}</td>
                </tr>
                <tr>
                    <th>Lindero sur</th>
                    <td>{{ entity.linderoSur }}</td>
                </tr>
                <tr>
                    <th>Lote terreno mt2</th>
                    <td>{{ entity.loteTerrenoMt2 }}</td>
                </tr>
                <tr>
                    <th>Nombre</th>
                    <td>{{ entity.nombre }}</td>
                </tr>
                <tr>
                    <th>Titularidad del terreno</th>
                    <td>{{ entity.titularidadDelTerreno }}</td>
                </tr>
                <tr>
                    <th>Total viviendas</th>
                    <td>{{ entity.totalViviendas }}</td>
                </tr>
                <tr>
                    <th>Total unidades</th>
                    <td>{{ entity.totalUnidades }}</td>
                </tr>
                <tr>
                    <th>Total viviendas protocolizadas</th>
                    <td>{{ entity.totalViviendasProtocolizadas }}</td>
                </tr>
                <tr>
                    <th>Urban. / Barrio</th>
                    <td>{{ entity.urbanBarrio }}</td>
                </tr>
                <tr>
                    <th>Zona</th>
                    <td>{{ entity.zona }}</td>
                </tr>
            </tbody>
        </table>
        <br>
        
        <div class="delete">
            <div class="row">
                <div class="col-md-2">
                        {% if delete_form is defined %}
                            {{ form(delete_form) }}
                        {% endif %} 
                </div>
                <div class="col-md-2">
                    <a href="{{ path('desarrollo_edit', { 'id': entity.id }) }}" class="btn btn-warning">Modificar</a>
                </div>
            </div>                
        </div>
    </div>
</div>

<div align="right">
    <a href="{{ path('desarrollo') }}" class="btn btn-success" role="button">Volver</a>
</div>

<br>            
{% endblock %}
