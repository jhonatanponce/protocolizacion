{% extends '::base.html.twig' %}

{% block body -%}
    {{ parent() }}
<div class="panel panel-default">
    
    <div class="panel-heading">
        <span class="glyphicon glyphicon-list-alt"></span>
        <h3 class="panel-title" style="display: inline;">{% if title is defined %}{{title}}{% endif %}</h3>
    </div>
    
    <div id="yw0" class="panel-body  table-responsive">
        
        <table class="records_list table table-bordered table-striped table-hover" border="1">
            <thead>
                <tr class="success">
                    <th>Id</th>
                    <th>Nombre</th>
                    <th>Descripción</th>
                    <th>Estatus Desarrollo</th>
                    <th>UrbanIzación-Barrio</th>
                    <th>Zona</th>
                    <th>Acción</th>
                </tr>
            </thead>
            <tbody>
        {% for entity in entities %}
                <tr>
                    <td><a href="{{ path('protocolizacion_desarrollo_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                    <td>{{ entity.nombre }}</td>
                    <td>{{ entity.descripcion }}</td>
                    <td>{{ entity.estatusDesarrolloId }}</td>
                    <td>{{ entity.urbanBarrio }}</td>
                    <td>{{ entity.zona }}</td>
                    <td>
                        <ul>
                            <li>
                                <a href="{{ path('protocolizacion_desarrollo_show', { 'id': entity.id }) }}">show</a>
                            </li>
                            <li>
                                <a href="{{ path('protocolizacion_desarrollo_edit', { 'id': entity.id }) }}">edit</a>
                            </li>
                        </ul>
                    </td>
                </tr>
        {% endfor %}
            </tbody>
        </table>
    </div>
</div>
<div align="right">
    <a href="{{ path('_welcome') }}" class="btn btn-success" role="button">Inicio</a>
    <a href="{{ path('protocolizacion_desarrollo_new') }}" class="btn btn-primary">Crear Registro</a>
</div>
    {% endblock %}
