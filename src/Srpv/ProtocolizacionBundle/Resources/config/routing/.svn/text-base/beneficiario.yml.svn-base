beneficiario:
    path:     /
    defaults: { _controller: "SrpvProtocolizacionBundle:Beneficiario:index" }

beneficiario_show:
    path:     /{id}/{message}/show
    defaults: { _controller: "SrpvProtocolizacionBundle:Beneficiario:show", message:0 }

beneficiario_consulta:
    path:     /{message}/consulta
    defaults: { _controller: "SrpvProtocolizacionBundle:Beneficiario:consulta", message:0 }

beneficiario_new:
    path:     /{nacionalidad}/{cedula}/new
    defaults: { _controller: "SrpvProtocolizacionBundle:Beneficiario:new" }

beneficiario_create:
    path:     /create
    defaults: { _controller: "SrpvProtocolizacionBundle:Beneficiario:create" }

beneficiario_edit:
    path:     /{id}/edit
    defaults: { _controller: "SrpvProtocolizacionBundle:Beneficiario:edit" }

beneficiario_delete:
    path:     /{id}/delete
    defaults: { _controller: "SrpvProtocolizacionBundle:Beneficiario:delete" }